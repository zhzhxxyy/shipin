{
  "name": "mpatch",
  "version": "0.1.2",
  "description": "Match and patch a file",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "prettier --write '**/*.{js,ts,md}'; standard --fix",
    "test": "mocha test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arniu/mpatch.git"
  },
  "keywords": [
    "match",
    "patch"
  ],
  "author": "Arniu Tseng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arniu/mpatch/issues"
  },
  "homepage": "https://github.com/arniu/mpatch#readme",
  "dependencies": {
    "glob": "^7.1.2"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "mocha": "^5.0.1",
    "prettier": "^1.10.2",
    "standard": "^11.0.0"
  },
  "lint-staged": {
    "*.{ts,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
