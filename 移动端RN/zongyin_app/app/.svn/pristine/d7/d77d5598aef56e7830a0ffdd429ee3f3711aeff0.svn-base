    yanSstr(num) {

        //pk10 1:#F0CA31 2.#22A7FF 3.#333333 4.#FF8533 5.#3AF9F9
          // 6.#335CFF 7.#CCCCCC  8.#FF4111 9.#801A1A 10.#17C739  0:9966ff
        let yansStr= sys.mainColor
        if (num==0) {
            yansStr = '#9966ff'
        }
        if (num==1 || num%10==1) {
            yansStr = '#F0CA31'
        }
        if (num==2 || num%10==2) {
            yansStr = '#22A7FF'
        }
        if (num==3 || num%10==3) {
            yansStr = '#525252'
        }
        if (num==4 || num%10==4) {
            yansStr = '#FF8533'
        }
        if (num==5 || num%10==5) {
            yansStr = '#3AF9F9'
        }
        if (num==6 || num%10==6) {
            yansStr = '#335CFF'
        }
        if (num==7 || num%10==7) {
            yansStr = '#CCCCCC'
        }
        if (num==8 || num%10==8) {
            yansStr = '#FF4111'
        }
        if (num==9 || num%10==9) {
            yansStr = '#801A1A'
        }
        if (num==10 || num%10==0) {
            yansStr = '#17C739'
        }

        return yansStr;
    }



