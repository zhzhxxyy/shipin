                    <Text style={{marginLeft:5,marginTop:(zhongHeight-20)/2,textAlignVertical:'center',height:20,width:20,fontSize: 15,color:sys.titleColor,backgroundColor:sys.mainColor,textAlign:'center',


                    }}>{hz%2?'单':'双'}</Text>

                </View>

            </View>
        )

    }
