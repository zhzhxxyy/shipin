                        ios: { lineHeight: zhongHeight},
                        android: {}
                    })

                }}>{expect}</Text>
                <Text style={{marginLeft:1,textAlignVertical:'center',height:zhongHeight,width:qitaW,fontSize: 15,color:sys.titleColor,backgroundColor:sys.backgroundColor,textAlign:'center',

                    ...Platform.select({
                        ios: { lineHeight: zhongHeight},
                        android: {}
                    })

                }}>{opencode}</Text>
                <Text style={{marginLeft:1,textAlignVertical:'center',height:zhongHeight,width:qitaW,fontSize: 15,color:sys.titleColor,backgroundColor:sys.backgroundColor,textAlign:'center',
                    ...Platform.select({
                        ios: { lineHeight: zhongHeight},
