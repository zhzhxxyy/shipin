        <LinearLayout
            android:id="@+id/ll_version"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:background="@android:color/white"
            android:layout_width="match_parent"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_height="50dp">

            <com.duomizhibo.phonelive.widget.FontIconView
                android:text="@string/icon_shezhi3"
                android:layout_width="30dp"
                android:gravity="center"
                android:textColor="#333333"
                android:textSize="25sp"
                android:layout_height="30dp" />
            <TextView
                android:id="@+id/tv_version_old"
                android:text="版本（2.0.0）"
                android:textSize="12sp"
                android:layout_marginLeft="5dp"
                android:textColor="@color/colorTextG4"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:singleLine="true"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/tv_version_new"
                android:text="有新版本"
                android:textSize="12sp"
                android:textColor="#ff0000"
                android:layout_marginRight="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:src="@drawable/me_go"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

