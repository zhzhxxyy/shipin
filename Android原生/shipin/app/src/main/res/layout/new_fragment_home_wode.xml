<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:scrollbars="none"
    android:background="#f5f5f5"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:background="@drawable/user_bg"
            android:layout_height="160dp">
            <View
                android:layout_alignParentBottom="true"
                android:background="@drawable/user_bg_2"
                android:layout_width="match_parent"
                android:layout_height="50dp"/>

            <com.duomizhibo.phonelive.widget.CircleImageView
                android:id="@+id/iv_head"
                android:src="@drawable/user_dafault_headimg"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:layout_width="80dp"
                android:layout_height="80dp" />

            <TextView
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/iv_head"
                android:id="@+id/tv_name"
                android:text="jason"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:maxLines="1"
                android:textSize="14sp"
                android:textColor="@android:color/white"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:id="@+id/ll_other"
                android:layout_below="@+id/tv_name"
                android:layout_centerHorizontal="true"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginTop="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <com.duomizhibo.phonelive.widget.FontIconView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/icon_jinbi2"
                    android:textColor="#E6BE41"
                    />
                <TextView
                    android:id="@+id/tv_jinbi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/white"
                    android:textSize="12sp"
                    android:layout_marginLeft="2dp"
                    android:text="12"
                    />
                <com.duomizhibo.phonelive.widget.FontIconView
                    android:layout_marginLeft="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/icon_vip2"
                    android:textColor="#E6BE41"
                    />
                <TextView
                    android:id="@+id/tv_vip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@android:color/white"
                    android:textSize="12sp"
                    android:layout_marginLeft="2dp"
                    android:text="您还不是VIP"
                    />
            </LinearLayout>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:background="#eee"
            android:layout_height="1dp"/>


        <LinearLayout
            android:background="@android:color/white"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="70dp">
            <View
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="1dp"/>
            <TextView
                android:id="@+id/tv_kamichongzhi"
                android:text="卡密充值"
                android:background="@drawable/btn_common_border_ef98a7"
                android:textColor="#ef98a7"
                android:gravity="center"
                android:textSize="12sp"
                android:layout_width="70dp"
                android:layout_height="30dp" />
            <View
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="1dp"/>
            <View
                android:layout_width="1dp"
                android:background="#eee"
                android:layout_height="30dp"/>
            <View
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="1dp"/>
            <TextView
                android:id="@+id/tv_chongzhi"
                android:text="充值金币/VIP"
                android:layout_margin="5dp"
                android:background="@drawable/btn_common_border_ef98a7"
                android:textColor="#ef98a7"
                android:gravity="center"
                android:textSize="12sp"
                android:layout_width="80dp"
                android:layout_height="30dp"/>

            <View
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="1dp"/>
            <View
                android:layout_width="1dp"
                android:background="#eee"
                android:layout_height="30dp"/>
            <View
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="1dp"/>
            <TextView
                android:id="@+id/tv_qiandao"
                android:text="签到"
                android:background="@drawable/btn_common_ef98a7"
                android:textSize="12sp"
                android:gravity="center"
                android:textColor="@android:color/white"
                android:layout_width="66dp"
                android:layout_height="30dp" />


            <View
                android:layout_width="0dp"
              android:layout_weight="1"
                android:layout_height="1dp"/>
        </LinearLayout>




        <LinearLayout
            android:id="@+id/ll_myshipin"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:background="@android:color/white"
            android:layout_width="match_parent"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_height="50dp">

            <com.duomizhibo.phonelive.widget.FontIconView
                android:text="@string/icon_video"
                android:layout_width="30dp"
                android:gravity="center"
                android:textColor="#333333"
                android:textSize="25sp"
                android:layout_height="30dp" />
            <TextView
                android:text="我的视频"
                android:textSize="12sp"
                android:layout_marginLeft="5dp"
                android:textColor="@color/colorTextG4"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content" />

            <ImageView
                android:src="@drawable/me_go"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:background="#eee"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_height="1dp"/>
        <LinearLayout
            android:id="@+id/ll_myshoucang"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:background="@android:color/white"
            android:layout_width="match_parent"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:layout_height="50dp">

            <com.duomizhibo.phonelive.widget.FontIconView
                android:text="@string/icon_info"
                android:layout_width="30dp"
                android:gravity="center"
                android:textColor="#333333"
                android:textSize="25sp"
                android:layout_height="30dp" />
            <TextView
                android:text="我的收藏"
                android:textSize="12sp"
                android:layout_marginLeft="5dp"
                android:textColor="@color/colorTextG4"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content" />

            <ImageView
                android:src="@drawable/me_go"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:background="#eee"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_height="1dp"/>
        <LinearLayout
